.form__recovery-text {
  margin-top: 48px;
  font-size: 14px;
  line-height: 1.2;
  color: #0F0F0F;
}



/* COOKIES */

.cookies {
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 5;
  width: 100%;
  padding-top: 68px;
  padding-left: 5%;
  padding-right: 6%;
  height: 508px;
  display: flex;
  flex-direction: column;
  background-color: #0F0F0F;
  transition: bottom 0.3s ease-out;
