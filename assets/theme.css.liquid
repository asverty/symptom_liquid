/* ROUND SIZES */

.round-sizes {
  width: 100%;
  margin-bottom: 7%;
  display: grid;
  grid-template-columns: repeat(auto-fill, 48px);
  grid-gap: 25px;
}

.round-sizes__size {
  position: relative;
}

.round-sizes__true-radio {
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
}

.round-sizes__fake-radio {
  width: 48px;
  height: 48px;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 1px solid #0F0F0F;
  border-radius: 50%;
}

@media screen and (max-width: 1580px) {
  .round-sizes__fake-radio {
    width: 44px;
    height: 44px;
  }
}

.round-sizes__fake-radio:hover {
  cursor: pointer;
}

.round-sizes__fake-radio::before {
  position: absolute;
  top: 0;
  left: 0;
  width: 48px;
  height: 48px;
  content: "";
  opacity: 0;
  background-image: url({{ 'size_cross.png' | asset_img_url: 'master' }});
  background-position: center;
  background-repeat: no-repeat;
  background-size: contain;
}

@media screen and (max-width: 1580px) {
  .round-sizes__fake-radio::before {
    width: 44px;
    height: 44px;
  }
}

.round-sizes__true-radio:checked +
.round-sizes__fake-radio::before {
  opacity: 1;
}

.round-sizes__text {
  font-size: 16px;
}

.item__variants-padding {
  padding-top: 4px;
  padding-right: 1px;
