  grid-template-columns: repeat(auto-fill, 86px);
  grid-gap: 12px;
}

.oval-sizes__size {
  position: relative;
}
.oval-sizes__true-radio {
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
}

.oval-sizes__fake-radio {
  width: 86px;
  height: 48px;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 16px;
  text-transform: uppercase;
  border: 1px solid #0F0F0F;
  border-radius: 33px;
}

@media screen and (max-width: 1580px) {
  .oval-sizes__fake-radio {
    height: 44px;
  }
}

.oval-sizes__fake-radio:hover {
