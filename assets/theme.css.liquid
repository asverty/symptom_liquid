}

.oval-sizes__fake-radio::before {
  position: absolute;
  top: 0;
  left: 0;
  width: 86px;
  height: 48px;
  content: "";
  opacity: 0;
  background-image: url({{ 'sides_cross.png' | asset_img_url: 'master' }});
  background-position: center;
