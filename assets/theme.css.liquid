  background-size: contain;
}

@media screen and (max-width: 1580px) {
  .oval-sizes__fake-radio::before {
    height: 44px;
  }
}

.oval-sizes__true-radio:checked +
.oval-sizes__fake-radio::before {
  opacity: 1;
}



/* HEADER */

.mobileShow {
  display: none;
}

.header {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 64px;
  padding-left: 3%;
  padding-right: 3%;
  display: grid;
  grid-template-columns: 1fr 2fr 1fr 2fr;
  z-index: 11;
}

.header__fraction {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  color: #fff;
}

.header--black .header__fraction{
  color: #0f0f0f; 
}

.header__fraction:last-child {
  justify-content: flex-end;
}

.header__logo {
  width: 139px;
  height: 41px;
  background-image: url({{ 'logo-black.png' | asset_img_url: '276x82' }});
  background-size: contain;
  background-position: center;
  background-repeat: no-repeat;
}

.header:not(.header--black) .header__logo_white {
  background-image: url({{ 'logo-white.png' | asset_img_url: '276x82' }});
}

.header__button {
  position: relative;
  margin-right: 2.5vw;
  font-size: 16px;
  text-decoration: none;
  color: #0F0F0F;
  background-color: transparent;
  outline: none;
  border: none;
  cursor: pointer;
}

.header__button:last-child {
  margin-right: 0;
}

.header:not(.header--black) .header__button.header__button_white {
  color: #FFFFFF;
}

.header:not(.header--black) .header__button.header__button_white span {
  color: #FFFFFF;
}

.header__button-outline {
  position: absolute;
  top: -13px;
  left: -12px;
  z-index: 3;
  width: calc(100% + 24px);
  height: calc(100% + 24px);
  border: 1px solid #0F0F0F;
  border-radius: 50%;
}

.header:not(.header--black) .header__button-outline_white {
  border: 1px solid #FFFFFF;
}

.header__button-outline_hidden {
  display: none;
}



/* MENUS: BRANDS, CATEGORIES */

.menu {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 10;
  width: 100%;
  height: 572px;
  padding-top: 64px;
  display: grid;
  grid-template-columns: 1fr 1fr;
  background-color: #FFFFFF;
  transition: visibility 0.3s, opacity 0.3s ease-out;
}

@media screen and (max-width: 1900px) {
  .menu {
    height: 444px;
  }
}

.menu_hidden {
  visibility: hidden;
  opacity: 0;
}

.menu__left-half {
  margin-top: 48px;
  padding-left: 3vw;
  display: grid;
  grid-template-columns: 1fr 1fr 1fr;
}

@media screen and (max-height: 880px) {
  .menu__left-half {
    margin-top: 36px;
  }
}

.menu__list-element {
  width: max-content;
  list-style: none;
  margin-bottom: 18px;
}

.menu__list-element:last-child {
  margin-bottom: 0;
}

.menu__link {
  font-size: 18px;
}

@media screen and (max-width: 1900px) {
  .menu__link {
    font-size: 18px;
  }
}

@media screen and (max-width: 1346px) {
  .menu__link {
    font-size: 16px;
  }
}

.menu__right-half {
  position: relative;
  width: 100%;
  height: 100%;
}

.menu__image {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  visibility: visible;
  opacity: 1;
  transition: visibility 0.3s, opacity 0.3s ease-out;
}

.menu__image_hidden {
  visibility: hidden;
  opacity: 0;
}



/* FORMS: LOG IN, SIGN IN, PASSWORD RECOVERY, SUBSCRIBE */

.form {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 5;
  width: 100%;
  height: 592px;
  padding-top: 114px;
  padding-right: 3%;
  padding-left: 3%;
  background-color: #FFFFFF;
  transition: visibility 0.3s, opacity 0.3s ease-out;
}

.customer-form {
  padding-top: 114px;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 2;
  width: 100%;
  height: 592px;
  padding-top: 114px;
  padding-right: 3%;
  padding-left: 3%;
  background-color: #FFFFFF;
}

.form_hidden {
  visibility: hidden;
  opacity: 0;
}

.form__grid {
  display: grid;
