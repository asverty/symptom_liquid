@font-face {
  font-family: "Nimbus Sans";
  src: url('{{"NimbusSanL-Reg.ttf" | asset_url }}') format("truetype"),
  url('{{"NimbusSanL-Reg.woff" | asset_url }}') format("woff"),
  url('{{"NimbusSanL-Reg.woff2" | asset_url }}') format("woff2");
  font-weight: 400;
  font-style: normal;
}


/* DEFAULT STYLES */

* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

body {
  position: relative;
  background-color: #FFFFFF;
}

main {
  min-height: 100vh;
  background-color: #FFFFFF;
  overflow: auto;
  -webkit-overflow-scrolling: auto;
}

.template-cart main{
  overflow: initial;
}

h1, h2, h3, h4, h5, h6, p,
span, li, a, input, button, div {
  font-family: "Nimbus Sans", Arial, sans-serif;
  font-style: normal;
  font-weight: 400;
  line-height: 1.2;
  text-decoration: none;
  color: #0F0F0F;
}


input, button {
  border: none;
  outline: none;
  background-color: transparent;
}

input::placeholder {
  color: #BDBDBD;
}

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

input[type=number] {
  -moz-appearance: textfield;
}

button:hover, a:hover {
  cursor: pointer;
}



/* TEXT PAGES */

.heading {
  font-size: 64px;
  text-transform: uppercase;
}

@media screen and (max-width: 1900px) {
  .heading {
    font-size: 48px;
  }
}

.heading_form {
  margin-bottom: 56px;
  margin-right: auto;
}

.heading_404 {
  margin-top: 30%;
  margin-left: auto;
  margin-right: 102px;
  font-size: 288px;
  line-height: 0.8;
  user-select: none;
  -ms-user-select: none;
  -moz-user-select: none;
  -khtml-user-select: none;
  -webkit-user-select: none;
  -webkit-touch-callout: none;
}

@media screen and (max-width: 1420px) {
  .heading_404 {
    font-size: 252px;
  }
}

.heading_text-page {
  padding-top: 98px;
  padding-bottom: 48px;
  padding-left: 3%;
  padding-right: 3%;
}

.heading_account {
  padding-top: 98px;
  padding-bottom: 28px;
  padding-left: 3%;
  padding-right: 3%;
}

.paragraph {
  padding-bottom: 16px;
  font-size: 14px;
}

.paragraph_form {
  margin-bottom: 32px;
}

.paragraph_cookies {
  font-size: 16px;
  text-transform: uppercase;
  color: #FFFFFF;
}

@media screen and (max-width: 1900px) {
  .paragraph_cookies {
    font-size: 14px;
  }
}

.paragraph_404 {
  margin-top: 32%;
  font-size: 36px;
}

.paragraph_order {
  font-size: 16px;
}

.paragraph__link {
  font-size: inherit;
  text-decoration: underline;
}



